<%= form_for Advancesearch.new , :html=> { id: 'peopleSearchForm' } do |f|   %>

  <ol>
    <li class="keywords">
      <label for="advs-keywords">Keywords</label>
      <%= f.text_field :keywords, :id=>"advs-keywords" %>
    </li>
    
    <li class="firstName">
        <label for="advs-firstName">First Name</label>
        <%= f.text_field :fname, :id=>"advs-firstName" %>
    </li>
    
    <li class="lastName">
      <label for="advs-lastName">Last Name</label>
      <%= f.text_field :lname, :id=>"advs-lastName" %>
    </li>
    
    <li class="title">
      <label for="advs-title">Email</label>
      <%= f.text_field :email, :id=>"advs-title" %>
    </li>

    <li class="title">
      <label for="advs-title">Title</label>
      <%= f.text_field :title, :id=>"advs-title" %>
    </li>
    
    <li class="company">
      <label for="advs-company">Company</label>
      <%= f.text_field :company, :id=>"advs-company" %>
    </li>
    
    <li class="school">
      <label for="advs-school">School</label>
      <%= f.text_field :school, :id=>"advs-school" %>
    </li>
    
    <li class="country">
      <label for="advs-countryCode">Country</label>
      <%= f.text_field :country, :id=>"advs-title" %>
    </li>

    <li class="country">
      <div class="actions">
        <%= f.submit 'Search' %>
      </div>
    </li>
  </ol>
  
<% end %>
